# OpenRouter API key — get from OpenRouter dashboard: https://openrouter.ai
OPENROUTER_API_KEY=
# Ollama local endpoint — default http://localhost:11434 if running Ollama locally
OLLAMA_API_ENDPOINT=
# Ollama embedding model — e.g. nomic-embed-text
OLLAMA_EMBEDDING_MODEL=
# Local data dir for Eliza's embedded DB — set a writable path
PGLITE_DATA_DIR=

# Required - Wallet Configuration (use ONE of the following)
# Private key (base58) — export from your Solana wallet (USE DEV/TEST KEYS ONLY HERE)
SOLANA_PRIVATE_KEY=
# OR public key (base58) — from your wallet address
SOLANA_PUBLIC_KEY=

# Optional - RPC Configuration
# RPC URL — from your provider (Helius/QuickNode/Ankr/etc.) or public RPC
SOLANA_RPC_URL=
# Helius API key — from Helius dashboard if using Helius RPC
HELIUS_API_KEY=

# Optional - Market Data
# Birdeye API key — from Birdeye developer portal
BIRDEYE_API_KEY=

# Optional - AI Service
# OPENAI_API_KEY=  # From OpenAI; only if you enable enhanced parsing

# Wallet public address (same as SOLANA_PUBLIC_KEY) used by agents for on-chain ops
SOL_ADDRESS=

# Trading parameters
# Slippage in percent for swaps (e.g. 5)
SLIPPAGE=
# Optional model overrides for OpenRouter
# OPENROUTER_SMALL_MODEL=
# OPENROUTER_LARGE_MODEL=
# OPENROUTER_IMAGE_MODEL=

# ElevenLabs API key — get from ElevenLabs dashboard: https://elevenlabs.io
ELEVENLABS_API_KEY=